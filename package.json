    "@mdi/font": "^5.9.55",
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "jquery": "^3.6",
    "laravel-mix": "^6.0.6",
